<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>preview</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>Guimrz.SimpleMapper</PackageId>
		<Version>1.0.0-preview</Version>
		<Authors>José Guimarães</Authors>
		<Description>Simple and lightweight object mapping library for .NET</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/guimrz/SimpleMapper</PackageProjectUrl>
		<RepositoryUrl>https://github.com/guimrz/SimpleMapper</RepositoryUrl>
		<PackageTags>object-mapping, lightweight, dotnet, library</PackageTags>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.4" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\SimpleMapper.Abstractions\SimpleMapper.Abstractions.csproj" />
	</ItemGroup>

</Project>
